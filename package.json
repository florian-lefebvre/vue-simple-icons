{
  "name": "vue-two-simple-icons",
  "version": "0.2.0",
  "license": "MIT",
  "author": {
    "name": "Florian LEFEBVRE",
    "email": "contact@florian-lefebvre.dev",
    "url": "https://florian-lefebvre.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/florian-lefebvre/vue-two-simple-icons"
  },
  "description": "Simple Icons as Vue components.",
  "main": "dist/vue-two-simple-icons.ssr.js",
  "browser": "dist/vue-two-simple-icons.esm.js",
  "module": "dist/vue-two-simple-icons.esm.js",
  "unpkg": "dist/vue-two-simple-icons.min.js",
  "files": [
    "dist/*",
    "src/lib-components/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "node build",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "publish": "npm publish"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "cross-env": "^7.0.3",
    "fs-extra": "^9.1.0",
    "minimist": "^1.2.5",
    "pascal-case": "^3.1.2",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "simple-icons": "^4.18.0",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12",
    "written-number": "^0.9.1"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "engines": {
    "node": ">=12"
  }
}
